{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "accadf"
		},
		"adls_linked_serive_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_linked_serive'"
		},
		"adls_linked_serive_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfstorageaccountv.dfs.core.windows.net/"
		},
		"git_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"link_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "accadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_linked_serive_accountKey": {
						"value": "[parameters('adls_linked_serive_accountKey')]"
					},
					"adls_linked_serive_properties_typeProperties_url": {
						"value": "[parameters('adls_linked_serive_properties_typeProperties_url')]"
					},
					"git_linked_service_properties_typeProperties_url": {
						"value": "[parameters('git_linked_service_properties_typeProperties_url')]"
					},
					"link_git_properties_typeProperties_url": {
						"value": "[parameters('link_git_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "accadf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'accadf_ArmTemplate_0')]"
			]
		}
	]
}